{
  "manifest_version": 2,
  "name": "Google Calendar gray weekends",
  "version": "0.1",
  "icons": { "16": "icons/icon16.png",
             "48": "icons/icon48.png",
            "128": "icons/icon128.png" },
  "content_scripts": [
    {
      "matches": ["https://calendar.google.com/*"],
      "js": ["content.js"],
      "run_at": "document_end"
    }
  ]
}
